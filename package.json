{
  "name": "tw-aria",
  "type": "module",
  "version": "1.0.2",
  "description": "A Tailwind CSS plugin that adds support for ARIA attributes",
  "author": "Miquel de Domingo",
  "license": "MIT",
  "homepage": "https://github.com/mikededo/tw-aria",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikededo/tw-aria.git"
  },
  "bugs": "https://github.com/mikededo/tw-aria/issues",
  "keywords": [
    "tailwindcss",
    "aria"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "test": "vitest --watch=false --globals"
  },
  "peerDependencies": {
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@changesets/cli": "^2.27.11",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "eslint": "^9.18.0",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.4"
  }
}
